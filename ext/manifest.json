{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["storage","activeTab", "tabs"],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
        "run_at": "document_idle",
      "js": ["content.js"]
    }
  ],
    "manifest_version": 2
  }
